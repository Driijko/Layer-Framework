x === 0) {\r\n            return cloneElement(child, {\r\n                interactivity: interactivity,\r\n                onClick: handleClick,\r\n                onMouseEnter: handleMouseEnter,\r\n                onMouseLeave: handleMouseLeave,\r\n            });\r\n        }\r\n        else {\r\n            return cloneElement(child, {\r\n                interactivity: interactivity,\r\n                triggerDeselect: triggerDeselect,\r\n            });\r\n        };\r\n    });\r\n\r\n\r\n    // RENDER //////////////////////////////////////////////////////////////\r\n    return (\r\n        modifiedChildren\r\n    );\r\n};\r\n\r\nexport default InteractiveElement;","C:\\Users\\DoubleD\\Desktop\\Code\\2021\\004 Layer Framework\\version1\\src\\App\\Content\\2 Elements\\Links\\Link Types\\SectionLink.js",["241"],"// IMPORTS ///////////////////////////////////////////////////////////////\r\n// Import libraries ----------------------------------------------------\r\nimport {useEffect} from \"react\";\r\n\r\n// COMPONENT ///////////////////////////////////////////////////////\r\nfunction SectionLink({\r\n    interactivity, linkTo, triggerExit, triggerDeselect\r\n}) {\r\n    \r\n    // TRIGGER PAGE EXIT //////////////////////////////////////////////\r\n    useEffect(()=> {\r\n        if (interactivity === \"selected\") {\r\n            triggerExit(linkTo);\r\n        };\r\n    },[interactivity]);\r\n\r\n    // RENDER //////////////////////////////////////////////////////////////\r\n    return (\r\n        <>\r\n        </>\r\n    );   \r\n};\r\n\r\n// EXPORTS /////////////////////////////////////////////////////////////\r\nexport default SectionLink;","C:\\Users\\DoubleD\\Desktop\\Code\\2021\\004 Layer Framework\\version1\\src\\App\\Content\\2 Elements\\Links\\Link Styles\\LinkStyle0.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\004 Layer Framework\\version1\\src\\App\\Content\\1 Sections\\Page2\\Page2.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\004 Layer Frame